{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        " prettier/prettier": "error",
        "no-unused-vars":"warn" ,
        "node/no-unpublished-require":0,
        "node/no-missing-require":0,
        "allowNamedFunctions": true
    }
}
